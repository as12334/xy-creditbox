<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.1.xsd"
       default-autowire="byName">
    <bean id="lotteryResultInitJob" class="so.wwb.creditbox.schedule.job.LotteryResultProduceJob" />
    <bean id="lotteryResultUserDefinedJob" class="so.wwb.creditbox.schedule.job.LotteryResultUserDefinedJob" />
    <bean id="lotteryResultSubJob" class="so.wwb.creditbox.schedule.job.LotteryResultSubJob" />
    <bean id="lotteryGatherTask"   class="so.wwb.creditbox.schedule.task.LotteryGatherTask"  scope="prototype"/>



</beans>
