    <!--根据玩家账号查询玩家信息(忽略大小写) PS:只查询正式账号 -->
    <select id="queryIgnoreCaseUserNamesExists" resultType="so.wwb.creditbox.model.manager.user.po.SysUserExtend" parameterType="string">
        SELECT * FROM sys_user_extend u LEFT JOIN user_player p ON u.id = p.id
        WHERE u.user_type= '23'
        AND p.mode='1'
        AND LOWER(u.username) in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            LOWER(#{item})
        </foreach>
    </select>

    <!--检查用户是否是已存在的正式账号-->
    <select id="checkUsernameByMode" parameterType="so.wwb.creditbox.model.manager.user.so.SysUserExtendSo" resultType="int">
        SELECT COUNT (*) FROM sys_user_extend WHERE username=#{username} AND id IN (SELECT id FROM user_player WHERE mode='1')
    </select>

    <!--仅查询正式账号-->
    <select id="queryUserByRankId" parameterType="so.wwb.creditbox.model.manager.user.so.SysUserExtendSo" resultMap="TABLE:sys_user_extend">
        SELECT ue.id, ue.username
        FROM sys_user_extend ue, user_player up
        WHERE ue."id" = up."id"
        AND ue.status = '1'
        AND up.mode = '1'
        AND up.rank_id = #{rankId}
    </select>

