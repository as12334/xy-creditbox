<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="so.wwb.creditbox.data.manager.lottery.SiteLotteryOddMapper">

    <resultMap id="TABLE:site_lottery_odd" type="so.wwb.creditbox.model.manager.lottery.po.SiteLotteryOdd">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="site_id" property="siteId"/>
        <result column="hid" property="hid"/>
        <result column="code" property="code"/>
        <result column="bet_code" property="betCode"/>
        <result column="play_code" property="playCode"/>
        <result column="bet_num" property="betNum"/>
        <result column="odd_a" property="oddA"/>
        <result column="odd_b" property="oddB"/>
        <result column="odd_c" property="oddC"/>
        <result column="min_odd" property="minOdd"/>
        <result column="max_odd" property="maxOdd"/>
        <result column="up_odd" property="upOdd"/>
        <result column="total_money" property="totalMoney"/>
        <result column="proportion" property="proportion"/>
        <result column="account_count" property="accountCount"/>
        <result column="odd_close" property="oddClose"/>
    </resultMap>

    <!--//region your codes 1-->


    <!--查询一条站点下赔率-->
    <select id="getOddBySiteId" parameterType="so.wwb.creditbox.model.manager.lottery.po.SiteLotteryOdd" resultType="Integer">
        select id from site_lottery_odd  where site_id=#{siteId} limit 1;
    </select>
    <!--//endregion your codes 1-->

</mapper>